{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "palette": {
    "os": "#d6deeb",
    "text_light": "#e8ecf4",
    "text_gray": "#414141",
    "text_dark": "#011627",
    "blue": "#00c7fc",
    "cyan": "#1bd4cd",
    "gray": "#575656",
    "green": "#3ced86",
    "lavender": "#b4befe",
    "orange": "#fe9259",
    "pink": "#fd7777",
    "red_muted": "#c8a3ba",
    "cmake": "#1e9748",
    "go": "#7fd5ea",
    "node": "#8ed072",
    "python": "#ffde57",
    "ruby": "#ff4b60",
    "rust": "#beb3ab"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "p:os",
          "template": " {{ if .WSL }}\ue712 on {{ end }}<#fff>{{ .Icon }}</> ",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "p:blue",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:cyan",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:pink{{ end }}",
            "{{ if gt .Ahead 0 }}p:green{{ end }}",
            "{{ if gt .Behind 0 }}p:lavender{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "diamond",
          "foreground": "p:cyan",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_muted{{ end }}"
          ],
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "p:cyan",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_muted{{ end }}"
          ],
          "template": "\uf252 {{ .FormattedMs }} ",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:node"
        },
        {
          "type": "go",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \u202d\ue626 {{ .Full }} ",
          "foreground": "p:go"
        },
        {
          "type": "python",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \ue73c {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }} ",
          "foreground": "p:python",
          "properties": {
            "display_mode": "context"
          }
        },
        {
          "type": "ruby",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "p:ruby",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          }
        },
        {
          "type": "cmake",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \ue61e \ue61d cmake {{ .Full }} ",
          "foreground": "p:cmake"
        },
        {
          "type": "rust",
          "style": "powerline",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " \ue7a8 {{ .Full }} ",
          "foreground": "p:rust"
        },
        {
          "type": "time",
          "style": "diamond",
          "invert_powerline": true,
          "foreground": "p:text_light",
          "template": " <#fff>\ue641</> {{ .CurrentDate | date .Format }} ",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          }
        }
      ]
    }
  ]
}
